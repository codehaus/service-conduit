<definitions
	xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd"
	xmlns:wsp="http://www.w3.org/ns/ws-policy" xmlns:wsp1_2="http://schemas.xmlsoap.org/ws/2004/09/policy"
	xmlns:wsam="http://www.w3.org/2007/05/addressing/metadata" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
	xmlns:tns="urn:org.sca4j:tests" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns="http://schemas.xmlsoap.org/wsdl/" targetNamespace="urn:org.sca4j:tests"
	name="jaxbService">
	<types>
		<xs:schema>
			<xs:element name="echoJaxb" type="tns:echoJaxb" />
			<xs:element name="echoJaxbResponse" type="tns:echoJaxbResponse" />
			<xs:element name="echoMessage" type="tns:echoMessage" />
			<xs:complexType name="echoJaxb">
				<xs:sequence>
					<xs:element name="arg0" type="tns:echoMessage"
						minOccurs="0" />
				</xs:sequence>
			</xs:complexType>
			<xs:complexType name="echoMessage">
				<xs:sequence>
					<xs:element name="info" type="xs:string" minOccurs="0" />
					<xs:element name="name" type="xs:string" minOccurs="0" />
				</xs:sequence>
			</xs:complexType>
			<xs:complexType name="echoJaxbResponse">
				<xs:sequence>
					<xs:element name="return" type="tns:echoMessage"
						minOccurs="0" />
				</xs:sequence>
			</xs:complexType>
		</xs:schema>
	</types>
	<message name="echoJaxb">
		<part name="parameters" element="tns:echoJaxb" />
	</message>
	<message name="echoJaxbResponse">
		<part name="parameters" element="tns:echoJaxbResponse" />
	</message>
	<portType name="jaxbService">
		<operation name="echoJaxb">
			<input wsam:Action="urn:org.sca4j:tests:jaxbService:echoJaxbRequest"
				message="tns:echoJaxb" />
			<output wsam:Action="urn:org.sca4j:tests:jaxbService:echoJaxbResponse"
				message="tns:echoJaxbResponse" />
		</operation>
	</portType>
	<binding name="Binding" type="tns:jaxbService">
		<soap:binding transport="http://schemas.xmlsoap.org/soap/http"
			style="document" />
		<operation name="echoJaxb">
			<soap:operation soapAction="" />
			<input>
				<soap:body use="literal" />
			</input>
			<output>
				<soap:body use="literal" />
			</output>
		</operation>
	</binding>
	<service name="jaxbService">
		<port name="" binding="tns:Binding">
			<soap:address location="http://localhost:8900/jaxbService" />
		</port>
	</service>
</definitions>