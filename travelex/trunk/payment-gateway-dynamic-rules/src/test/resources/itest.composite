<?xml version="1.0" encoding="UTF-8"?>
<composite xmlns="http://www.osoa.org/xmlns/sca/1.0"
           xmlns:sca4j="urn:sca4j.org"
           xmlns:sca="http://www.osoa.org/xmlns/sca/1.0"
           name="DynamicRuleSubmissionProcessorITest"
           autowire="false">

    <component name="DynamicRuleSubmissionProcessorITest">
    	<sca4j:junit class="com.travelex.tgbp.rules.dynamic.capture.DynamicRuleSubmissionProcessorITest" />
        <reference name="dynamicRuleSubmissionProcessor" target="DynamicRules/dynamicRuleSubmissionProcessor" />
    </component>
    
    <component name="DynamicRuleResolverITest">
    	<sca4j:junit class="com.travelex.tgbp.rules.dynamic.execute.DynamicRuleExecutionITest" />
        <reference name="dynamicRules" target="DynamicRules/dynamicRules" />
    </component>

    <component name="DynamicRules">
        <implementation.composite scdlResource="payment-gateway-dynamic-rules.composite"/>
        <reference name="dataStore" target="DummyDataStore"/>
    </component>

    <component name="DummyDataStore">
        <implementation.java  class="com.travelex.tgbp.rules.dynamic.capture.DummyDataStore"/>        
    </component>
           

</composite>


       
