<?xml version="1.0" encoding="UTF-8"?>
<composite xmlns="http://www.osoa.org/xmlns/sca/1.0"
           xmlns:sca4j="urn:sca4j.org"
           xmlns:sca="http://www.osoa.org/xmlns/sca/1.0"
           name="IntegrationAssemblyITest"
           autowire="false">

    <component name="IntegrationAssemblyITest">
    	<sca4j:junit class="com.travelex.tgbp.integration.IntegrationITest" />
        <reference name="schemaModelBuilder" target="IntegrationAssembly/schemaModelBuilder" />
    </component>

    <component name="IntegrationAssembly">
        <implementation.composite scdlResource="pg-integration-assembly.composite"/>
        
		<service name="schemaModelBuilder">
            <sca4j:binding.http />
        </service>
        
        <service name="dynamicRuleSubmissionProcessor">
            <sca4j:binding.http />
        </service>
        
        <service name="submissionQueryProcessor">
            <sca4j:binding.http />
        </service>
        
        <service name="outputConfigManagement">
            <sca4j:binding.http />
        </service>
        
        <service name="outputTriggerService">
            <sca4j:binding.http />
        </service>
        
        <property name="inputSubmissionBaseDir">/</property>
        <property name="outputSubmissionBaseDir">c:/tgbp-outbound-files</property>
        <property name="initialStartupDelayInSeconds">6</property>
        <property name="outputFrequencyInSeconds">10</property>
    </component>
        
</composite>


       
